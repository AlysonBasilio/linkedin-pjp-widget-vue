{
  "name": "linkedin-pjp-widget-vue",
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "build": "vue-tsc && vite build && vue-tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "publish": "npm publish --access public"
  },
  "files": [
    "dist"
  ],
  "main": "dist/linkedin-pjp-widget.umd.js",
